{
    "name": "lsaraujo/boilerplate-simple-api",
    "type": "project",
    "require": {
        "pecee/simple-router": "5.3.0.5",
        "php-di/php-di": "^7.0",
        "doctrine/orm": "^2.16",
        "vlucas/phpdotenv": "^5.5",
        "symfony/cache": "^6.3",
        "ramsey/uuid-doctrine": "^2.0",
        "lcobucci/jwt": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Henrique Araujo"
        }
    ],
    "scripts": {
        "start": "php -S 127.0.0.1:8000 -t public/"
    },
    "config": {
        "process-timeout": 0
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3"
    }
}
